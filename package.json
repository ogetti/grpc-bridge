{
  "name": "grpc-bridge",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "packageManager": "pnpm@9.0.0",
  "scripts": {
    "dev": "./src-tauri/target/release/grpc-bridge",
    "build": "pnpm vite build",
    "preview": "pnpm vite preview",
    "tauri:dev": "pnpm tauri dev",
    "tauri:build": "pnpm tauri build",
    "lint": "pnpm eslint .",
    "cross-build": "./cross-build.sh",
    "setup": "pnpm install && rustup target add x86_64-pc-windows-gnu x86_64-apple-darwin",
    "clean": "rm -rf dist/ src-tauri/target/ node_modules/",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "type-check": "tsc --noEmit",
    "deps:update": "pnpm update --latest",
    "deps:check": "pnpm audit",
    "release:patch": "pnpm version patch && git push && git push --tags",
    "release:minor": "pnpm version minor && git push && git push --tags",
    "release:major": "pnpm version major && git push && git push --tags"
  },
