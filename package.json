{
  "name": "grpc-bridge",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "packageManager": "pnpm@9.0.0",
  "scripts": {
    "dev": "pnpm tauri dev",
    "dev:frontend": "vite dev",
    "build": "pnpm vite build",
    "cross-build": "./cross-build.sh",
    "lint": "pnpm eslint .",
    "setup": "./setup-dev.sh",
    "clean": "rm -rf dist/ src-tauri/target/ node_modules/",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "type-check": "tsc --noEmit",
    "deps:update": "pnpm update --latest",
    "deps:check": "pnpm audit",
    "release:patch": "pnpm version patch && git push && git push --tags",
    "release:minor": "pnpm version minor && git push && git push --tags",
    "release:major": "pnpm version major && git push && git push --tags"
  },
  "dependencies": {
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@tanstack/react-query": "^5.50.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "i18next": "^25.5.2",
    "i18next-browser-languagedetector": "^8.2.0",
    "lucide-react": "^0.378.0",
    "react": "^18.3.0",
    "react-dom": "^18.3.0",
    "react-hot-toast": "^2.4.1",
    "react-i18next": "^15.7.3",
    "tailwind-merge": "^3.3.1",
    "zustand": "^4.5.0"
  },
  "devDependencies": {
    "@tauri-apps/api": "^2.0.0-rc",
    "@tauri-apps/cli": "^2.0.0-rc",
    "@types/node": "^24.3.1",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "@vitejs/plugin-react": "^5.0.2",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.0.0",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.4.0",
    "vite": "^5.2.0"
  }
}